{
  "name": "code-splitting-example",
  "version": "1.0.0",
  "description": "A toy app demonstrating how to do code splitting in Webpack.",
  "main": "server.js",
  "scripts": {
    "build": "NODE_ENV=production npx parcel build ./src/index.html --no-cache",
    "start": "npm run build && node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/malchata/code-splitting-example.git"
  },
  "keywords": [
    "webpack",
    "code",
    "splitting",
    "javascript"
  ],
  "author": "Jeremy L. Wagner <jeremy.l.wagner@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/malchata/code-splitting-example/issues"
  },
  "homepage": "https://github.com/malchata/code-splitting-example#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-emotion": "^9.2.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "emotion": "^9.2.4",
    "express": "^4.16.3",
    "parcel-bundler": "^1.9.3",
    "preact": "^8.2.9",
    "preact-async-route": "^2.2.0",
    "preact-emotion": "^9.2.4",
    "preact-router": "^2.6.1",
    "prop-types": "^15.6.2",
    "regenerator-runtime": "^0.12.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "safari >= 9"
            ]
          },
          "modules": false
        }
      ]
    ],
    "plugins": [
      [
        "transform-react-jsx",
        {
          "pragma": "h"
        }
      ],
      "syntax-dynamic-import"
    ]
  }
}
